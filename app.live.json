{
  "expo": {
    "sdkVersion": "21.0.0",
    "name": "Mantra",
    "description": "Mantra",
    "slug": "mantra",
    "privacy": "public",
    "version": "0.7.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app.png",
    "loading": {
      "icon": "./assets/icons/loading.png",
      "hideExponentText": false
    },
    "packagerOpts": {
