    "ios": {
      "bundleIdentifier": "com.charliejackson.mantra",
      "supportsTablet": true,
      "isRemoteJSEnabled": true,
      "icon": "./assets/icons/app-store-image.png"
    },
    "android": {
      "package": "com.charliejackson.mantra"
    }
  }
}
